
public class AutenticacaoUtil {
	private int senha;

	@Override
	public void setSenha(int senha) {
		// TODO Auto-generated method stub

	}

	@Override
	public boolean autentica(int senha) {
		// TODO Auto-generated method stub
		if (this.senha == senha) {
			return true;
		}

		return false;

	}

}
