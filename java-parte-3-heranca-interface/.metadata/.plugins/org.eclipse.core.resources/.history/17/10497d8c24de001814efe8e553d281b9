
public class Administrador extends Funcionario implements Autenticavel {
	
	private int senha;

	@Override
	public void setSenha(int senha) {
		// TODO Auto-generated method stub

	}

	@Override
	public boolean autentica(int senha) {
		// TODO Auto-generated method stub
		if (this.senha == senha) {
			return true;
		}

		return false;

	}

	@Override
	public double getBonificacao() {
		// TODO Auto-generated method stub
		return 50;
	}
	
	
	
	
}

