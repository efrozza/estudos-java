
public class TestaValores {
	public static void main(String[] args) {
		int primeiro = 5;
		int segundo = 7;

		// passagem por valor
		segundo = primeiro;
		primeiro = 10;

		System.out.println(segundo);
		
		char valor = 65;
		valor = (char) (valor + 1);         // Compila!
        System.out.println(valor);

	}
}
